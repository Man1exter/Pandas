    "import pandas as pd\n",
    "\n",
    "df = pd.DataFrame(dict(\n",
    "    A = [1,2,3],\n",
    "    B = [4,5,6],\n",
    "    C = [7,8,9]\n",
    "))\n",
    "\n",
    "df['col_total =>'] = df.apply(lambda x: x.sum(), axis = 1)\n",
    "df.loc['row_total =>'] = df.apply(lambda x: x.sum())\n",
    "\n",
    "print(df)"
   ]
